import java.util.*;
class ArrayCount
{
  public static void main(String args[])
  {
    int pro, e_sum=0,o_sum=0,n;
    Scanner sc = new Scanner(System.in);
    System.out.print("Enter size of array");
    n=sc.nextInt();
    int a[]=new int[n];
    System.out.print("Enter array's elements");
    for(int i=0;i<n;i++)
    a[i]=sc.nextInt();
    for(int j=0;j<n;j++)
    {
      if(j%2==0)   //Basically, it's just sum of all indexes. n/2 gives even and odd numbers.
      e_sum+=j;    // But if the index is 7 or some odd number,
      else           // we wouldn't know the exact number of odd/even indexes correctly.  
      o_sum+=j;  
    }
    pro=e_sum*odd_sum;
    if(pro%2==0)
    System.out.print("EVEN");
    else
    System.out.print("ODD");
  }
}
