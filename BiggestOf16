import java.util.*;
class Biggestone
{
 public static void main(String args[])
  { int a,b,c,d=0;
    Scanner sc=new Scanner(System.in);
    for(int i=1;i<=8;i++)
    {
      a=sc.nextInt();
      b=sc.nextInt();
      c=(a>b)?a:b;
      d=c;
      if(c>d)
      d=c;
    }System.out.println(d);
  } 
 }
