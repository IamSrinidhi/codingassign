import java.util.*;
class Fact 
{   
    public static void main(String args[])
    {
      Scanner sc = new Scanner(System.in);
      System.out.println("enter a number");
      int num=sc.nextInt();
      int fl= fact(num);
 System.out.println("Factorial : "+fl);
    }
static int fact(int n)
    {
        int f= 1;
        if(n==1)
        {  return 1;}
       f= fact(n-1)*n;
       return f;
    }
} 
