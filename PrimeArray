import java.util.*;
class PrimeArray1
{
  public static void main(String args[])
  {   int n,i,count=0;
    Scanner sc = new Scanner(System.in);
    System.out.print("Enter size of array");
    n=sc.nextInt( );
    int a[ ]=new int[n];
    for(i=1;i<=n;i++)
       a[i]=sc.nextInt();
    for(i=0;i<=Math.sqrt(a[i]);i++)
    {
       if(a[i]%i !=0)
       count++;
     }   
    System.out.print("Prime Numbers"+count);
  } 
}
     
