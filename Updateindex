import java.util.*;
class Updateindex
{
  public static void main(String args[])
  {
    ArrayList <Integer> a=new  ArrayList <Integer>();
    Scanner s = new Scanner(System.in);
    int n,num,rep;
    System.out.print("Enter size of ArrayList");
    n=s.nextInt();
    System.out.print("Enter elements");
    for(int i=0;i<n;++i)
    a.add(s.nextInt());
    System.out.print("Enter the number and the no. you want to get it replaced it with");
    num=s.nextInt();
    rep=s.nextInt();
    if(a.contains(num))
   {a.set(num,rep);
    for(int j:a)
    System.out.print(j+" ");
    }
  else  
  System.out.print("False");
    
  }
}
