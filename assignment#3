import java.util.Scanner;
class Grammar
{
  public static void main(String args[])
  {
    char hey;
    Scanner gram=new Scanner(System.in);
    hey=gram.next().charAt(0);
    switch(hey)
    {
      case 'a':
      case 'A':
      case 'e':
      case 'E':
      case 'i':
      case 'I':
      case 'o':
      case 'O':
      case 'u':
      case 'U':
                       System.out.println("vowel");
                       break;
      default:System.out.println("consonant");                 
    }  
  }
}
