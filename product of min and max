class Solution{
    
    // Function for finding maximum and value pair
    public static long find_multiplication (int arr[], int brr[], int n, int m) {
        long max=arr[0];long min=brr[0];long pro=1;
        int i;
        for(i=0;(i<n);i++)
        {
            if(arr[i]>max)
            max=arr[i];
        }
        for(i=0;(i<m);i++)
        {   if(brr[i]<min)
            min=brr[i];
        }
        pro=max*min;
        return pro;
        
    }
    
    
}
