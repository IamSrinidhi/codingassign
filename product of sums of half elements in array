class Complete{
    
   
    // Function for finding maximum and value pair
    public static int multiply (int arr[], int n) {
       int i,sum1=0,sum2=0;
       for(i=0;i<n/2;i++)
       {
         sum1+=arr[i];   
       }
       for(i=n/2;i<n;i++)
       {
           sum2+=arr[i];
       }
       return sum1*sum2;
       
    }
    
    
}
